<?xml version="1.0" encoding="utf-8"?>

<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.spanishlanguageappforbeginners">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity
            android:name="android.example.spanishlearningapp.AnimalsActivity"
            android:label="Animals" />

        <activity
            android:name="android.example.spanishlearningapp.ColorsActivity"
            android:label="Colors" />

        <activity
            android:name="android.example.spanishlearningapp.FamilyActivity"
            android:label="Family" />

        <activity
            android:name="android.example.spanishlearningapp.NumbersActivity"
            android:label="Numbers" />

        <activity
            android:name="android.example.spanishlearningapp.PhrasesActivity"
            android:label="Phrases" />

        <activity android:name="android.example.spanishlearningapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>



    </application>

</manifest>